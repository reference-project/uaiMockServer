com.uaihebert.uaimockserver {
	port = 2223
	host = localhost
	context = /uaiMockServer
	fileLog = true
	consoleLog = false
	defaultContentTypeResponse = "application/json;charset=UTF-8"
	routes = [
	    {
			request {
				path = "/"
				method = "GET"
			}
			response {
				contentType = "application/json;charset=UTF-8"
				statusCode = 200
				body = """{"mockBody":{"title":"Hello World"}}"""
			}
	    }
	]
}