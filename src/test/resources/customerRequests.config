com.uaihebert.uaimockserver {
	routes = [
	    {
			request {
				path = "/customer"
				method = "PUT"
				isBodyRequired = true
			}
			response {
				contentType = "application/json;charset=UTF-8"
				statusCode = 201
				body = """{"mockCustomerBody":{"name":"Joseph of Arimathea"}}"""
			}
	    }
	]
}